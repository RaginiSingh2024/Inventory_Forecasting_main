{
  "name": "inventory-forecasting-app",
  "version": "1.0.0",
  "description": "Inventory Forecasting Web Application - A comprehensive inventory management and demand forecasting system built with HTML, CSS, JavaScript, and Firebase",
  "main": "frontend/index.html",
  "scripts": {
    "start": "cd frontend && npm start",
    "dev": "cd frontend && npm run dev",
    "serve": "cd frontend && npm run serve",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "deploy": "cd backend && npm run deploy",
    "deploy:hosting": "cd backend && npm run deploy:hosting",
    "deploy:firestore": "cd backend && npm run deploy:firestore",
    "emulators": "cd backend && npm run emulators:start",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "clean": "rm -rf frontend/node_modules backend/node_modules",
    "setup": "npm run install:all",
    "build": "echo 'Building frontend and backend...' && cd frontend && npm run build"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "keywords": [
    "inventory",
    "forecasting",
    "management",
    "dashboard",
    "firebase",
    "javascript",
    "html",
    "css",
    "charts",
    "analytics",
    "web-application",
    "demand-forecasting",
    "stock-management"
  ],
  "author": {
    "name": "Raj Koli",
    "email": "raj@example.com",
    "url": "https://github.com/Rajkoli145"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rajkoli145/inventory-mange.git"
  },
  "bugs": {
    "url": "https://github.com/Rajkoli145/inventory-mange/issues"
  },
  "homepage": "https://github.com/Rajkoli145/inventory-mange#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "private": false,
  "files": [
    "frontend/**/*",
    "backend/**/*",
    "README.md",
    "LICENSE"
  ]
}
