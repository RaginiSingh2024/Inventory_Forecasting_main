{
  "name": "inventory-forecasting-backend",
  "version": "1.0.0",
  "description": "Backend configuration for Inventory Forecasting Web Application - Firebase setup and deployment",
  "main": "index.js",
  "scripts": {
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "emulators:start": "firebase emulators:start",
    "emulators:ui": "firebase emulators:start --import=./emulator-data",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "serve": "firebase serve",
    "init": "firebase init",
    "login": "firebase login",
    "logout": "firebase logout",
    "projects": "firebase projects:list",
    "use": "firebase use --add",
    "functions:shell": "firebase functions:shell",
    "functions:log": "firebase functions:log",
    "test": "echo 'Add Firebase emulator tests'",
    "validate:rules": "firebase firestore:rules:validate firestore.rules"
  },
  "keywords": [
    "firebase",
    "firestore",
    "backend",
    "inventory",
    "forecasting",
    "database",
    "authentication",
    "hosting"
  ],
  "author": "Raj Koli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rajkoli145/inventory-mange.git"
  },
  "bugs": {
    "url": "https://github.com/Rajkoli145/inventory-mange/issues"
  },
  "homepage": "https://github.com/Rajkoli145/inventory-mange#readme",
  "dependencies": {
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0"
  },
  "devDependencies": {
    "firebase-tools": "^12.7.0",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true,
  "files": [
    "firebase.json",
    "firestore.rules",
    "functions/**/*"
  ]
}
